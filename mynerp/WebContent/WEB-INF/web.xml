<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <!--display-name>mynerp</display-name-->
  <display-name>other</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <login-config>
    <auth-method>FORM</auth-method>
    <realm-name>default</realm-name>
    <form-login-config>
      <form-login-page>/login.jsp</form-login-page>
      <form-error-page>/erroLogin.jsp</form-error-page>
    </form-login-config>
  </login-config>
  <security-constraint>
    <display-name>transcal</display-name>
    <web-resource-collection>
      <web-resource-name>Valores</web-resource-name>
      <url-pattern>/valores.jsp</url-pattern>
    </web-resource-collection>
    <auth-constraint>
      <role-name>LOGADO</role-name>
    </auth-constraint>
  </security-constraint>
  <security-constraint>
    <display-name>transcal</display-name>
    <web-resource-collection>
      <web-resource-name>Bem Vindo</web-resource-name>
      <url-pattern>/posLogin.jsp</url-pattern>
    </web-resource-collection>
    <auth-constraint>
      <role-name>LOGADO</role-name>
    </auth-constraint>
  </security-constraint>
  <security-constraint>
    <display-name>transcal</display-name>
    <web-resource-collection>
      <web-resource-name>testes</web-resource-name>
      <url-pattern>/testes.jsp</url-pattern>
    </web-resource-collection>
    <auth-constraint>
      <role-name>LOGADO</role-name>
    </auth-constraint>
  </security-constraint>
  <security-role>
    <description>Quando usuario estiver logado</description>
    <role-name>LOGADO</role-name>
  </security-role>
</web-app>